<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.laercio.organizze">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.Despesas.DespesasActivity"
            android:theme="@style/despesaTheme"></activity>
        <activity android:name=".ui.Receitas.ReceitasActivity"
            android:theme="@style/receitaTheme"/>
        <activity
            android:name=".ui.Principal.PrincipalActivity"
            android:label="@string/title_activity_principal"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ui.CadastroUsuario.CadastroActivity" />
        <activity android:name=".ui.login.LoginActivity" />
        <activity
            android:name=".ui.main.MainActivity"
            android:theme="@style/Theme.Intro">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>